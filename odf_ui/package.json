    "scripts": {
        "start": "run-script-os",
        "start:win32": "set PORT=8573 && react-scripts start",
        "start:default": "export PORT=8573 && react-scripts start",
        "build": "run-script-os",
        "build:default": "react-scripts --max_old_space_size=8192 build && rm -rf ../odf/web/static && mv build ../odf/web/static",
